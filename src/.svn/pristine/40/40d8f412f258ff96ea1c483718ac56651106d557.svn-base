const mapStateToProps = (state) => {
    return {
        NeetReducer: state.NeetReducer
    };
}

const mapDispatchToProps = (dispatch) => {
    return {
        ProfileDetailAction: (Token) => {
            dispatch(ProfileDetailAction(Token));
        },
        ProfileUpdateAction: (Token, StudentEmail, FatherMobile, MotherMobile, Address, Pincode, City, District, State) => {
            dispatch(ProfileUpdateAction(Token, StudentEmail, FatherMobile, MotherMobile, Address, Pincode, City, District, State));
        },

    }
}
export default connect(mapStateToProps, mapDispatchToProps)(MyProfile);